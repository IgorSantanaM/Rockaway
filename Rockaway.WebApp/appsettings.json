{
	"Logging": {
		"LogLevel": {
			"Default": "Information",
			"Microsoft": "Information",
			"Microsoft.EntityFrameworkCore": "Warning",
			"AspNetCore.SassCompiler": "Warning" 
		}
	},
	"AllowedHosts": "*",
	"SassCompiler": {
		"Source": "wwwroot/scss",
		"Target": "wwwroot/css",
		"Arguments": "--style=compressed",
		"Configurations": {
			"Debug": {
				"Arguments": "--style=expanded"
			}
		}
	}
}
